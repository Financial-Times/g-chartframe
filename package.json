{
  "name": "g-chartframe",
  "version": "5.1.12",
  "description": "FT styled frames for D3 graphics",
  "license": "ISC",
  "jsnext:main": "index.js",
  "main": "build/g-chartframe.js",
  "keywords": [
    "dom",
    "visualization",
    "svg",
    "d3"
  ],
  "repository": {
    "type": "git",
    "url": "https://github.com/Financial-Times/g-chartframe/"
  },
  "scripts": {
    "watch": "watch 'npm run build' ./src",
    "pretest": "npm run build",
    "build": "mkdir -p build && node -e 'process.stdout.write(\"var version = \\\"\" + require(\"./package.json\").version + \"\\\"; export * from \\\"../index\\\"; export {version};\");' > build/bundle.js && rollup -f umd -n gChartframe -o build/g-chartframe.js -- build/bundle.js",
    "test": "nyc --produce-source-map --require '@babel/register' tape test/*.js",
    "prepublishOnly": "npm run test",
    "report-coverage": "nyc report --reporter=lcov && codecov"
  },
  "homepage": "https://github.com/Financial-Times/g-chartframe/",
  "contributors": [
    "Tom Pearson <tom.g.pearson@gmail.com>",
    "Ã†ndrew Rininsland <aendrew@aendrew.com> (http://www.aendrew.com)",
    "Steve Bernard",
    "Bob Haslett",
    "Anna Leach"
  ],
  "devDependencies": {
    "@babel/core": "^7.0.0-beta.31",
    "@babel/preset-env": "^7.0.0-beta.31",
    "@babel/register": "^7.0.0-beta.31",
    "codecov": "^3.0.0",
    "d3": "^4.9.1",
    "eslint": "^4.11.0",
    "eslint-config-airbnb-base": "^12.1.0",
    "eslint-plugin-import": "^2.8.0",
    "jsdom": "^11.0.0",
    "nyc": "^11.3.0",
    "rollup": "0.25",
    "tape": "4",
    "uglify-js": "2",
    "watch": "^0.17.1"
  },
  "dependencies": {
    "save-svg-as-png": "^1.2.0"
  }
